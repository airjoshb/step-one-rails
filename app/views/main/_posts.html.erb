<% @posts.reverse.each do |post| %>
  <%= link_to update_path(post) do %>
    <div class="imageContainer">
      <%= cl_image_tag(post.image.key, width: 200, height: 200, crop: :fill, gravity: 'center', alt: post.title) %>
      <h5 class="shadowTitle"><%= post.title %></h5>
    </div>
  <% end %>
<% end %>
